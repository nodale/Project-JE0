
FoamFile
{
   version     2.0;
   format      ascii;
   class       dictionary;
   object      surfaceFeatureExtractDict;
}

blade.stl
{
   extractionMethod    extractFromSurface;
   extractFromSurfaceCoeffs
   {
       includedAngle 180;
   }

   subsetFeatures
   {
       nonManifoldEdges    yes;
       openEdges           yes;
   }
   writeObj    yes;
}
